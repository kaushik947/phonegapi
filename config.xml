<?xml version='1.0' encoding='utf-8'?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.gam.jam"
    versionCode = "10" 
    version     = "1.0.0" >
 <name>phonegapi</name>
    <description>
        A sample Apache Cordova application that authenticates with google services and uses google gapi.
    </description>
    <author email="rscohn2@gmail.com" href="http://cordova.io">
        RoCo Software
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>

    <feature name="StartApp">
        <param name="android-package" value="com/phonegap/plugins/startapp.StartApp" />
    </feature>
    <gap:plugin name="org.li8.inappbrowser" version="0.1" />
</widget>